                                    <![CDATA[#from ast import literal_eval
import json
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        #ALFrameManagerの所得
        self.framemanager=ALProxy("ALFrameManager");
        #初期化
        self.folderName=None
        pass

    def onUnload(self):
        #put clean-up code here
        import sys
        #初期化
        if self.folderName and self.folderName in sys.path:
            sys.path.remove(self.folderName)
        self.folderName=None
        pass

    def onInput_onStart(self,p):
        #self.onStopped() #activate the output of the box
        import sys,os
        #用意したライブラリまでのパスを所得
        self.folderName=os.path.join(self.framemanager.getBehaviorPath(self.behaviorId),"../lib")
        #ライブラリをインポート可能にするために、所得したパスを追加
        if self.folderName not in sys.path:
            sys.path.append(self.folderName)

        import requests
        from prettyprint import pp,pp_str
        #import json

        r=requests.get('http://api.openweathermap.org/data/2.5/forecast?q="Tokyo","Japan"'+'&APPID='+self.getParameter("APIKey"));
        list=r.json()['list']
        #list=r.json()
        #self.log(list)
        today=list[2]
        tommorow=list[10]
        '''
        result = {\
            "今日の天気は":getWeather(today),\
            "今日の温度は":getTempa(today),\
            "明日の天気は":getWeather(tommorow),\
            "明日の温度は":getTempa(tommorow)\
        }
        '''
        self.log(p)
        if p=='<...> 今日の天気 <...>':
            self.result(getWeather(today))
            self.weather(getWeatherTCP(today))
            self.startMotion()
        elif p=='<...> 今日の温度 <...>':
            self.result(getTempa(today))
            self.startMotion()
        elif p=='<...> 明日の天気 <...>':
            self.result(getWeather(tommorow))
            self.weather(getWeatherTCP(tommorow))
            self.startMotion()
        elif p=='<...> 明日の温度 <...>':
            self.result(getTempa(tommorow))
            self.startMotion()
        elif p=='<...> C3PO <...>' or p=='<...> c3po <...>' or p=='C3PO' or p=='c3po' or p=='<...> しーすりーぴーおー <...>':
            self.weather("c3po")
            self.c3po()
            self.startMotion()
        elif p=='<...> ばいばい <...>':
            self.onStopped()
        '''
        self.log(result)
        result=str(result).decode('string-escape')
        self.log(type(literal_eval(result)))
        self.log(pp_str(result))
        result=pp_str(result)
        self.log(result)
        self.result(result[p])
        '''
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

def getWeather(obj):
    weather=obj['weather'][0]['main'].replace('"','');
    if(weather=='Clear'):
        return '晴れです'
    elif(weather=='Clouds'):
        return '曇りです'
    elif (weather =='Rain'):
        return '雨です'
    else:
        return 'わかりません'
def getTempa(obj):
    temp=int(float(json.dumps(obj['main']['temp']).replace('"',''))-273.15)
    return str(temp)+'度です'
def getWeatherTCP(obj):
    weather=obj['weather'][0]['main'].replace('"','');
    if(weather=='Clear'):
        return 'clear'
    elif(weather=='Clouds'):
        return 'clouds'
    elif (weather =='Rain'):
        return 'rain'
    else:
        return 'none'
def getTempaTCP(obj):
    temp=int(float(json.dumps(obj['main']['temp']).replace('"',''))-273.15)
    return str(temp)]]>
